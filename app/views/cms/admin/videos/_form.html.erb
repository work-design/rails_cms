<%= form_with model: [:admin, @video], local: true do |f| %>
  <%= render 'shared/error_messages', target: @video %>
  <%= f.text_field :title %>
  <%= f.collection_select :video_taxon_id, VideoTaxon.all, :id, :name %>
  <%= f.file_field :media %>
  <%= f.file_field :cover %>
  <%= f.submit %>
<% end %>
